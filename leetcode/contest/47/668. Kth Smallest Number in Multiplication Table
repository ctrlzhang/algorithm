class Solution {
public:
    int findKthNumber(int m, int n, int k) {
        int low = 1, high = m * n;
        while(low < high) {
            int mid = low + (high - low) / 2;
            int cnt = count(mid, m, n);
            if(cnt < k) {
                low = mid + 1;
            } else {
                high = mid;
            }
        }
        
        return high;
    }
    
    int count(int k, int m, int n) {
        int ans = 0;
        for(int i = 1; i <= n; i++) {
            ans += min(k / i, m);
        }
        
        return ans;
    }
};
