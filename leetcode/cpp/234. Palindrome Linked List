/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
 /**
  * 思路: 空间复杂度o(n) 时间负责度o(n)
  */ 
class Solution {
public:
    bool isPalindrome(ListNode* head) {
        if(NULL == head) return true;
        
        vector<int> v;
        while(head != NULL) {
            v.push_back(head->val);
            head = head->next;
        }
        
        int i=0, j=v.size()-1;
        while(i < j) {
            if(v[i] != v[j]) return false;
            i++;
            j--;
        }
        
        return true;
    }
};
