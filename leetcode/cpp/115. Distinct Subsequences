class Solution {
public:
    unordered_map<string, int> mp;
    int numDistinct(string s, string t) {
        string key = s + "_" + t;
        if(mp.find(key) != mp.end()) return mp[key];
        
        //edge case
        if(s == t) return 1;
        if(t.empty()) return 1;
        if(s.empty()) return 0;
        
        int ans = 0;
        for(int i = 0; i < s.length(); i++) {
            if(t[0] == s[i]) {
                ans += numDistinct(s.substr(i + 1), t.substr(1));
            }
        }
        
        mp[key] = ans;
        return ans;
    }
};
