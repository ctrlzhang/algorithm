class Solution {
public:
    vector<int> countBits(int num) {
        vector<int> ans;
        for(int i=0; i<=num; i++) {
            int t = i;
            int count = 0;
            while(t > 0) {
                t = t & (t-1);
                count++;
            }
            
            ans.push_back(count);
        }
        
        return ans;
    }
};
